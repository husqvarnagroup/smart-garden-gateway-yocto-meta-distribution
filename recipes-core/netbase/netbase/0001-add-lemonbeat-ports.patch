---
 etc-services | 36 ++++++++++++++++++++++++++++++++++++
 1 file changed, 36 insertions(+)

diff --git a/etc-services b/etc-services
index a039d7e..904ada7 100644
--- a/etc-services
+++ b/etc-services
@@ -589,3 +589,39 @@ tfido		60177/tcp			# fidonet EMSI over telnet
